{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "string"
        },
        "userFirstName": {
          "type": "string"
        },
        "userLastName": {
          "type": "string"
        },
        "userMiddleName": {
          "type": "string"
        },
        "userPhoneNumber": {
          "type": "integer"
        },
        "userLocation": {
          "type": "string"
        },
        "userTimeZone": {
          "type": "string"
        },
        "userLinkedinUrl": {
          "type": "string"
        },
        "userEduUg": {
          "type": "string"
        },
        "userEduPg": {
          "type": "string"
        },
        "userComments": {
          "type": "string"
        },
        "userVisaStatus": {
          "type": "string"
        },
        "userLoginEmail": {
          "type": "string"
        }
      },
      "required": [
        "userId",
        "userFirstName",
        "userLastName",
        "userMiddleName",
        "userPhoneNumber",
        "userLocation",
        "userTimeZone",
        "userLinkedinUrl",
        "userEduUg",
        "userEduPg",
        "userComments",
        "userVisaStatus",
        "userLoginEmail"
      ]
    },
    "roles": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "roleId": {
              "type": "string"
            },
            "userRoleStatus": {
              "type": "string"
            }
          },
          "required": [
            "roleId",
            "userRoleStatus"
          ]
        }
      ]
    }
  },
  "required": [
    "user",
    "roles"
  ]
}
